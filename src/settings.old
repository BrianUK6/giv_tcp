class GiV_Settings:
    invertorIP="192.168.2.146"           #Required - IP address of Invertor on local network
    dataloggerSN="SA12345678"         #Required - Wifi/GPS S/N found here: https://www.givenergy.cloud/GivManage/setting/deviceMenu/inverterList
    output="MQTT"               #Required - "MQTT" or "JSON" (JSON will print JSON formatted string to stdout)
    MQTT_Address="192.168.2.83"         #Required (if output=MQTT) -  IP address of MQTT broker (local or remote)
    MQTT_Username="mqtt"        #Optional - Username for MQTT broker
    MQTT_Password="mqtt2020"        #Optional - Password for MQTT broker
    MQTT_Topic=""           #Optional - Root topic for all MQTT messages. Defaults to "GivEnergy/<dataloggerSN> 
    debug="True"                #Optional - Enables verbose debug "True" or "False".
    Debug_File_Location=""  #Optional - Location of debug file (Default is root directory)
    Print_Raw_Registers="False"  #Optional - "True" prints all raw registers to the MQTT broker